{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MyRedisConStr": "localhost:6379" // 연결 문자열은 무조건 ConnectionStrings 내부에 있어야 함
